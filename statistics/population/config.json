    "filters": [
        {
          "id": "calendarid",
          "label": "Calendrier",
          "type": "integer",
          "input": "select",
          "operators": ["equal", "not_equal", "is_null", "is_not_null"],
          "values" : {
              "table" : "apsolu_calendars",
              "fields" : "id,name",
              "sort" : "name"
          }
        },   
        {
          "id": "calendartypeid",
          "label": "Type de calendrier",
          "type": "integer",
          "input": "select",
          "operators": ["equal", "not_equal", "is_null", "is_not_null"],
          "values" : {
              "table" : "apsolu_calendars_types",
              "fields" : "id,name",
              "sort" : "name"
          }
        },        
        {
          "id": "cityid",
          "label": "Site",
          "type": "integer",
          "input": "select",
          "operators": ["equal", "not_equal", "is_null", "is_not_null"],
          "values" : {
              "table" : "apsolu_cities",
              "fields" : "id,name",
              "sort" : "name"
          }
        },
        {
          "id": "userid",
          "label": "Identifiant APSOLU",
          "type": "string",
          "operators": ["equal", "not_equal", "is_null", "is_not_null"]
        },        
        {
          "id": "idnumber",
          "label": "Numéro d'étudiant",
          "type": "string",
          "operators": ["equal", "not_equal"]
        },
        {
          "id": "firstname",
          "label": "Prénom",
          "type": "string",
          "operators": ["equal", "not_equal", "is_null", "is_not_null"]
        },                
        {
          "id": "lastname",
          "label": "nom",
          "type": "string",
          "operators": ["equal", "not_equal", "is_null", "is_not_null"]
        },
        {
          "id": "email",
          "label": "Courriel",
          "type": "string",
          "operators": ["equal", "not_equal"]
        },
        {
          "id": "sexe",
          "label": "Sexe",
          "type": "string",
          "input": "select",
          "operators": ["equal", "not_equal"],
          "values" : {
              "table" : "user_info_data",
              "fields" : "data",
              "conditions" : "fieldid=2",
              "sort" : "data"
          }
        },
        {
          "id": "institution",
          "label": "Institution",
          "type": "string",
          "input": "select",
          "values" : {
              "table" : "user",
              "fields" : "institution",
              "conditions" : "institution<>''",
              "sort" : "institution"
          }
        },
        {
          "id": "department",
          "label": "Département",
          "type": "string",
          "input": "select",
          "operators": ["equal", "not_equal", "in", "not_in", "is_null", "is_not_null"],
          "values" : {
              "table" : "user",
              "fields" : "department",
              "sort" : "department"
          }
        },
        {
          "id": "ufr",
          "label": "UFR",
          "type": "string",
          "input": "select",
          "operators": ["equal", "not_equal"],
          "values" : {
              "table" : "user_info_data",
              "fields" : "data",
              "conditions" : "fieldid=4",
              "sort" : "data"
          }          
        },
        {
          "id": "lmd",
          "label": "Cycle LMD",
          "type": "string",
          "input": "select",
          "operators": ["equal", "not_equal"],
          "values" : {
              "table" : "user_info_data",
              "fields" : "data",
              "conditions" : "fieldid=5",
              "sort" : "data"
          }          
        },
        {
          "id": "roleshortname",
          "label": "Type d'inscription",
          "type": "string",
          "input": "select",
          "operators": ["contains","not_contains","equal", "not_equal"],
          "values" : {
              "table" : "role",
              "fields" : "shortname",
              "conditions" : "archetype = 'student' and shortname <> 'student'",
              "sort" : "shortname"
          }          
        },
        {
          "id": "enrolname",
          "label": "Inscriptions (semestre)",
          "type": "string",
          "input": "select",
          "operators": ["equal", "not_equal"],
          "values" : {
              "table" : "enrol",
              "fields" : "name",
              "conditions" : "enrol = 'select'",
              "sort" : "name"
          }          
        },          
        {
          "id": "apsolucardpaid",
          "label": "Carte sport",
          "type": "string",
          "input": "select",
          "operators": ["equal", "not_equal"],
          "values" : {
              "Oui":"Oui",
              "Non":"Non"
          }          
        },
        {
          "id": "status",
          "label": "Liste (statut inscription)",
          "type": "integer",
          "input": "select",
          "values" : {
              "0" : "Liste des étudiants acceptés",
              "2" : "Liste principale",
              "3" : "Liste complémentaire",
              "4" : "Liste des étudiants désinscrits"
          }          
        },        
        {
          "id": "groupid",
          "label": "Groupements d'activités",
          "type": "integer",
          "input": "select",
          "operators": ["equal", "not_equal", "is_null", "is_not_null"],
          "values" : {
              "table" : "course_categories",
              "fields" : "id,name",
              "conditions" : "parent = 0",
              "sort" : "name"
          }
        },
        {
          "id": "activityid",
          "label": "Activité",
          "type": "integer",
          "input": "select",
          "operators": ["equal", "not_equal", "is_null", "is_not_null"],
          "values" : {
              "table" : "course_categories",
              "fields" : "id,name",
              "conditions" : "parent > 0",
              "sort" : "name"
          }
        },
        {
          "id": "userprofile",
          "label": "Type",
          "type": "integer",
          "input": "select",
          "operators": ["equal", "not_equal", "is_null", "is_not_null"],
          "values" : {
            "Inconnu":"Inconnu",
            "Étudiant":"Étudiant",
            "Personnel":"Personnel"
          }
        }
                        
        
    ],
    "filtersCustomRennes": [
        {
          "id": "shnu",
          "label": "Sportif de haut niveau",
          "type": "boolean",
          "input": "select",
          "operators": ["equal", "not_equal", "is_null", "is_not_null"],
          "values" : {
