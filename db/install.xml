            <FIELD NAME="activityid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" /> <!-- TODO: est-ce toujours utile ? -->
            <FIELD NAME="locationid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" />
            <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" />
            <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" />
        </FIELDS>
        <KEYS>
            <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for apsolu_attendance_sessions" />
        </KEYS>
        <INDEXES>
            <INDEX NAME="courseid" UNIQUE="false" FIELDS="courseid" />
            <INDEX NAME="activityid" UNIQUE="false" FIELDS="activityid" />
            <INDEX NAME="locatoinid" UNIQUE="false" FIELDS="locationid" />
        </INDEXES>
    </TABLE>

